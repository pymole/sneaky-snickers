{
    "bots": {
        "v1": {
            "type": "from_commit",
            "build": {
                "dir": "./build_dir/v1",
                "commit": "v1",
                "flags": ["--release"]
            },
            "mute": true,
            "launch": "target/release/sneaky-snickers"
        },
        "current": {
            "type": "unmanaged",
            "addresses": ["127.0.0.1:8000"]
        }
    },

    "ports": {
        "from": 9000,
        "to": 9100
    },

    "rules": {
        "build_dir": "./build_dir"
    },

    "arena": {

    }
}
